<h1>Edit Premise</h1>
 <div class="row">
   <div class="col-md-8">
     <%= form_for [@argument, @premise] do |f| %>
       <div class="form-group">
         <%= f.label :body%>
         <%= f.text_field :body, class: 'form-control', placeholder: "Enter premise text" %>
       </div>
       <%= f.submit "Edit Premise", class: 'btn btn-success', style: 'margin-bottom: 10px;'%>
     <% end %>
   </div>
 </div>